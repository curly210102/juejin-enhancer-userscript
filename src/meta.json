{
  "@name": "Juejin Extension Enhancer",
  "@name:zh-CN": "掘金扩展助手",
  "@description": "Enhances Juejin",
  "@description:zh-CN": "提供一个扩展平台，聚合掘金社区的各类第三方扩展，可插拔使用。扩展旨在为掘金社区提供辅助支持。",
  "@match": "*",
  "@run-at": "document-end",
  "@require": "tampermonkey://vendor/jquery.js",
  "@grant": [
    "GM_addStyle",
    "GM_addElement",
    "GM_log",
    "GM_setValue",
    "GM_getValue",
    "GM_deleteValue",
    "GM_listValues",
    "GM_addValueChangeListener",
    "GM_removeValueChangeListener",
    "GM_getResourceText",
    "GM_getResourceURL",
    "GM_registerMenuCommand",
    "GM_unregisterMenuCommand",
    "GM_openInTab",
    "GM_xmlhttpRequest",
    "GM_download",
    "GM_getTab",
    "GM_saveTab",
    "GM_getTabs",
    "GM_notification",
    "GM_setClipboard"
  ],
  "@connect": [
    "juejin.cn",
    "github.com",
    "gitee.com",
    "juejin-enhancer-extensions.vercel.app"
  ]
}
