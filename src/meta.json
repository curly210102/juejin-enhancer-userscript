{
  "@name": "Juejin Enhancer",
  "@name:zh-CN": "掘金助手",
  "@description": "Enhances Juejin",
  "@description:zh-CN": "掘金功能增强，提供进度追踪、数据统计、操作辅助等功能。",
  "@match": "https://juejin.cn/*",
  "@match": "https://localhost:3000/*",
  "@run-at": "document-end",
  "@require": "tampermonkey://vendor/jquery.js",
  "@grant": [
    "GM_addStyle",
    "GM_addElement",
    "GM_log",
    "GM_setValue",
    "GM_getValue",
    "GM_deleteValue",
    "GM_listValues",
    "GM_addValueChangeListener",
    "GM_removeValueChangeListener",
    "GM_getResourceText",
    "GM_getResourceURL",
    "GM_registerMenuCommand",
    "GM_unregisterMenuCommand",
    "GM_openInTab",
    "GM_xmlhttpRequest",
    "GM_download",
    "GM_getTab",
    "GM_saveTab",
    "GM_getTabs",
    "GM_notification",
    "GM_setClipboard"
  ],
  "@connect": ["juejin.cn", "github.com", "gitee.com"]
}
